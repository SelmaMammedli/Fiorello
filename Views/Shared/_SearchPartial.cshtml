@model IEnumerable<Product>


@foreach(var products in Model)
{
    <li>
        <a asp-controller="product" asp-action="detail" asp-route-id="@products.Id">
          <img style="border-radius:50%" width="50px" height="50px" src="~/img/@products.ProductImages?.FirstOrDefault(p=>p.IsMain)?.ImageUrl">  @products.Name @products.Price
        </a>
    </li>

}
